{{#partial "content" }}

    {{#each personName }}
    <h1 style="font-size: larger;">Welcome {{ name }}!</h1>
    {{/each }}
    <h1 style="font-size: larger;">Projects</h1>

<ul>
    {{#each projectList }}
        <li>{{ name }}: {{ description }} <br>
            Looking for {{ num_needed }} people willing to work {{time_commit}} hours per week<br>
            Posted by user {{ownerId}}
        </li>
    {{/each }}
</ul>

<br>
    <a href="/create">Create a Project</a>
    <br>
    <a href="/">Sign Out</a>
{{/partial }}

{{> base.hbs}}