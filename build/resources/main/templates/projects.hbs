{{#partial "content" }}
<h1>Projects</h1>

<ul>
    {{#each projectList }}
        <li>{{ name }}: {{ description }}</li>
    {{/each }}
</ul>

<form action="/projects" method="post">
    <input type="text" placeholder="project name" name="projectname"/>
    <input type="text" placeholder="project desc" name="projectdesc"/>
    <button>Submit</button>
</form>
{{/partial }}

{{> base.hbs}}